<div class="container">
  <div class="col-md-6 col-md-offset-3 col-sm-10 col-sm-offset-1">
<h3>Quote with the following contents</h3>

<%= form_for(@quotes) do |f| %>
  <%= f.hidden_field :content %>
  <p>Content  :  <%= @quotes.content %></p>
  <%= f.submit "Quote" ,class: 'btn btn-primary btn-block'  %>
<% end %>

<%= form_for(@quotes, url: new_quote_path, method: 'get') do |f| %>
  <%= f.hidden_field :content %>
  <%= f.submit "back",name:"back", class: 'btn btn-default btn-block' %>
<% end %>
  </div>
</div>